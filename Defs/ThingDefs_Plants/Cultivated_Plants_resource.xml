<?xml version="1.0" encoding="utf-8" ?>
<Defs>


	

	

  <ThingDef ParentName="VG_PlantDef">
    <defName>PlantSteelLeaf</defName>
    <label>steel shroom tree</label>
    <description>Plants with natural occurring steel in it's veins. Steel shrooms can be smelted into steel ore.</description>
    <graphicData>
      <texPath>Things/Plants/SteelPlant</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <plant>
      <harvestedThingDef>RawSteelLeaf</harvestedThingDef>
      <harvestYield>16</harvestYield>
      <sowMinSkill>8</sowMinSkill>
      <growDays>12</growDays>
      <fertilitySensitivity>1</fertilitySensitivity>
      <wildOrder>2</wildOrder>
      <purpose>Misc</purpose>
   <sowResearchPrerequisites>
        <li>resourceleafs</li>
      </sowResearchPrerequisites>
    </plant>
  </ThingDef>

